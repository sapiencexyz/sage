# Required for predictions
OPENAI_API_KEY=sk-your-api-key-here

# Optional - for submitting attestations on-chain
PRIVATE_KEY=
RPC_URL=https://base-mainnet.infura.io/v3/YOUR_KEY

# Optional - for different LLM providers
# ANTHROPIC_API_KEY=
# TOGETHER_API_KEY=

# Agent settings
MODEL_PROVIDER=openai
DEBUG=false

# Autonomous Mode Settings
AUTO_MODE_ENABLED=false            # Start in auto mode on launch
AUTO_INTERVAL=300000               # Loop interval in milliseconds (5 minutes)
AUTO_BATCH_SIZE=5                  # Number of markets to process per cycle
AUTO_MIN_CONFIDENCE=0.6            # Minimum confidence threshold (0-1)